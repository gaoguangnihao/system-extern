define sign-image-in-list
8 $(PRODUCT_OUT)/$(2):
9 $(1): $(PRODUCT_OUT)/$(2) $(3) $(4) $(5)
10 	@echo sign: $$<
11 	@if [ -f "$$<" ]; then \
12           PYTHONDONTWRITEBYTECODE=True \
13           PRODUCT_OUT=$(PRODUCT_OUT) \
14           BOARD_AVB_ENABLE=$(BOARD_AVB_ENABLE) \
15           python $(4) -target $(2) -env_cfg $(5) $$(MTK_PLATFORM_DIR) $$(MTK_BASE_PROJECT); \
16          else \
17           echo $$< does not exist, no need to sign!; \
18          fi
19 endef

